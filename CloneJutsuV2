local Player = owner
local Character = Player.Character
Character.Archivable = true
local CloneTemplate = Character:Clone()
for _,Part in pairs(CloneTemplate) do
  if Part.Name == CloneTemplate.Name then
    Part:Destroy()
  end
end
Character.Archivable = false
local RunService = game:GetService("RunService")
for x = 1,9 do
  local Offset = Vector3.new()
  if math.random(0,1) > 0 then
    if math.random(0,1) > 0 then
      Offset = Vector3.new(math.random(1,4) * 4,0,math.random(-4,4) * 4)
    else
      Offset = Vector3.new(math.random(-4,-1) * 4,0,math.random(-4,4) * 4)
    end
  else
    if math.random(0,1) > 0 then
      Offset = Vector3.new(math.random(-4,4) * 4,0,math.random(1,4) * 4)
    else
      Offset = Vector3.new(math.random(-4,4) * 4,0,math.random(-4,-1) * 4)
    end
  end
  local Clone = CloneTemplate:Clone()
  local conn = RunService.Heartbeat:Connect(function(x)
    local CloneTarget = Character.HumanoidRootPart.Position + Offset
    local YourY = CloneTarget.Y
    local MyY = Clone.HumanoidRootPart.Position.Y
    local DeltaY = YourY - MyY
    wait(x/20)
    if DeltaY > 0.2 then
      Clone.Humanoid.Jump = true
    end
    Clone.Humanoid:MoveTo(CloneTarget)
  end)
  Clone.Humanoid.Died:Connect(function()
    conn:Disconnect()
    local Debris = game:GetService("Debris")
    Debris:AddItem(Clone,1)
    wait(0.25)
    for k,v in pairs(Clone:GetDescendants()) do
      if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then
        for x = 1,2 do
          local Fire = Instance.new("Fire",v)
          Fire.Color = Color3.fromRGB(95,95,95)
          Fire.SecondaryColor = Color3.new()
          Fire.Size = 5
          Fire.Heat = 2
          spawn(function()
            wait(0.5)
            Fire.Enabled = false
          end)
        end
        spawn(function()
          for t = 0.1,1,0.1 do
            wait(0.05)
            v.Transparency = t
          end
        end)
      end
    end
  end)
  Clone.Parent = Character
  local CloneTarget = Character.HumanoidRootPart.Position + Offset
  Clone:MoveTo(CloneTarget)
  wait(1/20)
end
