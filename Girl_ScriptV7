local Player = owner
local Humanoid = Character:FindFirstChildOfClass("Humanoid")
local Phrases = {"Mmmmmmmmh...","I'm getting so wet down there...","I want these clothes off so bad..."}
while Humanoid.Health > 0 and Character ~= nil do
	wait(5)
	local Character = Player.Character
	local Chat = game:GetService("Chat")
	Chat:Chat(Character.PrimaryPart,Phrases[math.random(#Phrases)],Enum.ChatColor.White)
end
